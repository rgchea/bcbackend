{% extends 'BackendAdminBundle::layout.html.twig' %}


{% block body %}


    <form action="{{ path('backend_admin_company_index') }}" method="POST">
        <input type="hidden" name="submit">
        <button type="submit" class="btn btn-primary">
            <i class="fa fa-save"></i>
            {{ 'label_save'|trans  }}
        </button>


    </form>
    <script type="text/javascript" language="JavaScript">


        alert(calculateWeight(300, 100, 100, 6));
        
        function calculateWeight(long, width, height, kg) {
            //el largo, por el ancho y el alto (en cm) y el cubicaje (kilos entre metros cúbicos), y dividirlo entre 1 millón.
            weight = (parseFloat(long) *  parseFloat(width) * parseFloat(height) * parseFloat(kg) ) / 1000000;
            return Math.ceil(2.205*weight);
        }
        
    </script>

{% endblock %}
