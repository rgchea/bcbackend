<?php

namespace Backend\AdminBundle\Entity;

/**
 * AlliedOrderLog
 */
class AlliedOrderLog
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $actionLog;

    /**
     * @var string
     */
    private $description;

    /**
     * @var \DateTime
     */
    private $createdAt;

    /**
     * @var \Backend\AdminBundle\Entity\User
     */
    private $user;

    /**
     * @var \Backend\AdminBundle\Entity\AlliedOrder
     */
    private $alliedOrder;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set actionLog
     *
     * @param string $actionLog
     *
     * @return AlliedOrderLog
     */
    public function setActionLog($actionLog)
    {
        $this->actionLog = $actionLog;

        return $this;
    }

    /**
     * Get actionLog
     *
     * @return string
     */
    public function getActionLog()
    {
        return $this->actionLog;
    }

    /**
     * Set description
     *
     * @param string $description
     *
     * @return AlliedOrderLog
     */
    public function setDescription($description)
    {
        $this->description = $description;

        return $this;
    }

    /**
     * Get description
     *
     * @return string
     */
    public function getDescription()
    {
        return $this->description;
    }

    /**
     * Set createdAt
     *
     * @param \DateTime $createdAt
     *
     * @return AlliedOrderLog
     */
    public function setCreatedAt($createdAt)
    {
        $this->createdAt = $createdAt;

        return $this;
    }

    /**
     * Get createdAt
     *
     * @return \DateTime
     */
    public function getCreatedAt()
    {
        return $this->createdAt;
    }

    /**
     * Set user
     *
     * @param \Backend\AdminBundle\Entity\User $user
     *
     * @return AlliedOrderLog
     */
    public function setUser(\Backend\AdminBundle\Entity\User $user = null)
    {
        $this->user = $user;

        return $this;
    }

    /**
     * Get user
     *
     * @return \Backend\AdminBundle\Entity\User
     */
    public function getUser()
    {
        return $this->user;
    }

    /**
     * Set alliedOrder
     *
     * @param \Backend\AdminBundle\Entity\AlliedOrder $alliedOrder
     *
     * @return AlliedOrderLog
     */
    public function setAlliedOrder(\Backend\AdminBundle\Entity\AlliedOrder $alliedOrder = null)
    {
        $this->alliedOrder = $alliedOrder;

        return $this;
    }

    /**
     * Get alliedOrder
     *
     * @return \Backend\AdminBundle\Entity\AlliedOrder
     */
    public function getAlliedOrder()
    {
        return $this->alliedOrder;
    }
	
	
    public function setCreatedAtValue()
    {
        $this->createdAt = new \DateTime();
    }		
}
