<?php

namespace Backend\AdminBundle\Entity;

/**
 * Message
 */
class Message
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $subject = '';

    /**
     * @var string
     */
    private $message;

    /**
     * @var \DateTime
     */
    private $createdAt;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set subject
     *
     * @param string $subject
     *
     * @return Message
     */
    public function setSubject($subject)
    {
        $this->subject = $subject;

        return $this;
    }

    /**
     * Get subject
     *
     * @return string
     */
    public function getSubject()
    {
        return $this->subject;
    }

    /**
     * Set message
     *
     * @param string $message
     *
     * @return Message
     */
    public function setMessage($message)
    {
        $this->message = $message;

        return $this;
    }

    /**
     * Get message
     *
     * @return string
     */
    public function getMessage()
    {
        return $this->message;
    }

    /**
     * Set createdAt
     *
     * @param \DateTime $createdAt
     *
     * @return Message
     */
    public function setCreatedAt($createdAt)
    {
        $this->createdAt = $createdAt;

        return $this;
    }

    /**
     * Get createdAt
     *
     * @return \DateTime
     */
    public function getCreatedAt()
    {
        return $this->createdAt;
    }
    /**
     * @var \Backend\AdminBundle\Entity\User
     */
    private $sender;


    /**
     * Set sender
     *
     * @param \Backend\AdminBundle\Entity\User $sender
     *
     * @return Message
     */
    public function setSender(\Backend\AdminBundle\Entity\User $sender = null)
    {
        $this->sender = $sender;

        return $this;
    }

    /**
     * Get sender
     *
     * @return \Backend\AdminBundle\Entity\User
     */
    public function getSender()
    {
        return $this->sender;
    }
    /**
     * @var \Backend\AdminBundle\Entity\AlliedHouse
     */
    private $alliedHouse;


    /**
     * Set alliedHouse
     *
     * @param \Backend\AdminBundle\Entity\AlliedHouse $alliedHouse
     *
     * @return Message
     */
    public function setAlliedHouse(\Backend\AdminBundle\Entity\AlliedHouse $alliedHouse = null)
    {
        $this->alliedHouse = $alliedHouse;

        return $this;
    }

    /**
     * Get alliedHouse
     *
     * @return \Backend\AdminBundle\Entity\AlliedHouse
     */
    public function getAlliedHouse()
    {
        return $this->alliedHouse;
    }
	
    public function setCreatedAtValue()
    {
        $this->createdAt = new \DateTime();
    }	
	
	
    /**
     * @var \Backend\AdminBundle\Entity\AlliedHouseComplaintCategory
     */
    private $alliedHouseComplaintCategory;


    /**
     * Set alliedHouseComplaintCategory
     *
     * @param \Backend\AdminBundle\Entity\AlliedHouseComplaintCategory $alliedHouseComplaintCategory
     *
     * @return Message
     */
    public function setAlliedHouseComplaintCategory(\Backend\AdminBundle\Entity\AlliedHouseComplaintCategory $alliedHouseComplaintCategory = null)
    {
        $this->alliedHouseComplaintCategory = $alliedHouseComplaintCategory;

        return $this;
    }

    /**
     * Get alliedHouseComplaintCategory
     *
     * @return \Backend\AdminBundle\Entity\AlliedHouseComplaintCategory
     */
    public function getAlliedHouseComplaintCategory()
    {
        return $this->alliedHouseComplaintCategory;
    }
}
