<?php

namespace Backend\AdminBundle\Entity;

/**
 * ListPriceProduct
 */
class ListPriceProduct
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var float
     */
    private $price = '0.00';

    /**
     * @var \Backend\AdminBundle\Entity\Product
     */
    private $product;

    /**
     * @var \Backend\AdminBundle\Entity\ListPrice
     */
    private $listPrice;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set price
     *
     * @param float $price
     *
     * @return ListPriceProduct
     */
    public function setPrice($price)
    {
        $this->price = $price;

        return $this;
    }

    /**
     * Get price
     *
     * @return float
     */
    public function getPrice()
    {
        return $this->price;
    }

    /**
     * Set product
     *
     * @param \Backend\AdminBundle\Entity\Product $product
     *
     * @return ListPriceProduct
     */
    public function setProduct(\Backend\AdminBundle\Entity\Product $product = null)
    {
        $this->product = $product;

        return $this;
    }

    /**
     * Get product
     *
     * @return \Backend\AdminBundle\Entity\Product
     */
    public function getProduct()
    {
        return $this->product;
    }

    /**
     * Set listPrice
     *
     * @param \Backend\AdminBundle\Entity\ListPrice $listPrice
     *
     * @return ListPriceProduct
     */
    public function setListPrice(\Backend\AdminBundle\Entity\ListPrice $listPrice = null)
    {
        $this->listPrice = $listPrice;

        return $this;
    }

    /**
     * Get listPrice
     *
     * @return \Backend\AdminBundle\Entity\ListPrice
     */
    public function getListPrice()
    {
        return $this->listPrice;
    }
}
