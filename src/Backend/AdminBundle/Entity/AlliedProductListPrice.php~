<?php

namespace Backend\AdminBundle\Entity;

/**
 * AlliedProductListPrice
 */
class AlliedProductListPrice
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var float
     */
    private $price = '0.00';

    /**
     * @var \Backend\AdminBundle\Entity\Product
     */
    private $product;

    /**
     * @var \Backend\AdminBundle\Entity\User
     */
    private $user;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set price
     *
     * @param float $price
     *
     * @return AlliedProductListPrice
     */
    public function setPrice($price)
    {
        $this->price = $price;

        return $this;
    }

    /**
     * Get price
     *
     * @return float
     */
    public function getPrice()
    {
        return $this->price;
    }

    /**
     * Set product
     *
     * @param \Backend\AdminBundle\Entity\Product $product
     *
     * @return AlliedProductListPrice
     */
    public function setProduct(\Backend\AdminBundle\Entity\Product $product = null)
    {
        $this->product = $product;

        return $this;
    }

    /**
     * Get product
     *
     * @return \Backend\AdminBundle\Entity\Product
     */
    public function getProduct()
    {
        return $this->product;
    }

    /**
     * Set user
     *
     * @param \Backend\AdminBundle\Entity\User $user
     *
     * @return AlliedProductListPrice
     */
    public function setUser(\Backend\AdminBundle\Entity\User $user = null)
    {
        $this->user = $user;

        return $this;
    }

    /**
     * Get user
     *
     * @return \Backend\AdminBundle\Entity\User
     */
    public function getUser()
    {
        return $this->user;
    }
    /**
     * @var \Backend\AdminBundle\Entity\AlliedHouseUser
     */
    private $alliedHouseUser;


    /**
     * Set alliedHouseUser
     *
     * @param \Backend\AdminBundle\Entity\AlliedHouseUser $alliedHouseUser
     *
     * @return AlliedProductListPrice
     */
    public function setAlliedHouseUser(\Backend\AdminBundle\Entity\AlliedHouseUser $alliedHouseUser = null)
    {
        $this->alliedHouseUser = $alliedHouseUser;

        return $this;
    }

    /**
     * Get alliedHouseUser
     *
     * @return \Backend\AdminBundle\Entity\AlliedHouseUser
     */
    public function getAlliedHouseUser()
    {
        return $this->alliedHouseUser;
    }
}
