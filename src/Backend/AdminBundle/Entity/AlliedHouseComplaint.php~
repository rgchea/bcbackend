     */
    public function setAlliedHouseComplaintCateory(\Backend\AdminBundle\Entity\AlliedHouseComplaintCategory $alliedHouseComplaintCateory = null)
    {
        $this->alliedHouseComplaintCateory = $alliedHouseComplaintCateory;

