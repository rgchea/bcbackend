<?php

namespace Backend\AdminBundle\Entity;

/**
 * AlliedHouseComplaint
 */
class AlliedHouseComplaint
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $complaint;

    /**
     * @var \DateTime
     */
    private $createdAt = '0000-00-00';

