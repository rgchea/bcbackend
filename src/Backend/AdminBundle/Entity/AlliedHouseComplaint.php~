    }

    /**
     * Get alliedHouseComplaintCateory
     *
     * @return \Backend\AdminBundle\Entity\AlliedHouseComplaintCategory
     */
    public function getAlliedHouseComplaintCateory()
    {
        return $this->alliedHouseComplaintCateory;
    }
